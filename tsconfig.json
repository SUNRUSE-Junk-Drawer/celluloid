{
  "files": [
    "src/index.ts"
  ],
  "compilerOptions": {
    "outFile": "dist/index.js",
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "target": "es3",
    "sourceMap": true
  }
}
